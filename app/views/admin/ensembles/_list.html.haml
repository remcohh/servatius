- @ensembles.each do |ensemble|
  .row.mb-1.align-items-center.data-row
    .col-12.border-bottom
      .row
        .col-6
          = link_to ensemble.name, admin_ensemble_path(ensemble)
        .col-6
          .btn-group.float-right
            = link_to('Verwijderen', admin_ensemble_path(ensemble), method: :delete, data: { confirm: 'Weet u het zeker?' })

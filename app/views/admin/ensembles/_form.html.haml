.container-fluid
  .row
    .col-md-1
    .col-12.col-md-10
      = bootstrap_form_for ['admin', @ensemble] do |f|
        .row
          .col-12.col-md-10
            %h3
              Ensemble
        .row
          .col-12.col-md-10
            = f.text_field :name, label: 'Naam'
        .row
          .col-12.col-md-10
            = f.submit 'Opslaan', class: 'btn btn-primary'
    .col-md-1
